
//public String substring(int)
//public String substring(int, int)
public class StringAPIDemo06 {
	public static void main(String[] arg){
		String str = "mengzhaoyang";
		System.out.println("the str 4 to end is: "+ str.substring(4));
		System.out.println("the str 4 to 8 is: "+ str.substring(4,8));
	}
}
